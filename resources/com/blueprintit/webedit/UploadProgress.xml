<dialog resizable="false" layout="GridBagLayout" size="300,200">
	<label id="label" text="Label">
		<gridbagconstraints insets="5,10,5,10" gridx="0" gridy="0" weighty="1" weightx="1" fill="GridBagConstraints.HORIZONTAL"/>
	</label>
	<progressbar id="fileProgress">
		<gridbagconstraints insets="5,10,5,10" gridx="0" gridy="1" weighty="1" weightx="1" fill="GridBagConstraints.HORIZONTAL"/>
	</progressbar>
	<progressbar id="totalProgress">
		<gridbagconstraints insets="5,10,5,10" gridx="0" gridy="2" weighty="1" weightx="1" fill="GridBagConstraints.HORIZONTAL"/>
	</progressbar>
	<button text="Cancel">
		<gridbagconstraints insets="5,10,5,10" gridx="0" gridy="3" weighty="1" weightx="1" fill="GridBagConstraints.NONE" anchor="GridBagConstraints.CENTER"/>
	</button>
</dialog>
